<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>tutaj wprowadź wyświetlaną nazwę</displayName>
  <description>tutaj wprowadź opis</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Podgląd zdarzeń</string>
      <string id="EventViewer_RedirectionProgram">Program events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Jest to program, który zostanie wywołany po kliknięciu przez użytkownika łącza events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Parametry wiersza polecenia programu events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Określa parametry wiersza polecenia, które zostaną przekazane do programu events.asp.</string>
      <string id="EventViewer_RedirectionURL">Adres URL events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Jest to adres URL, który zostanie przekazany do obszaru Opis w oknie dialogowym Właściwości zdarzenia. Użytkownik może zmienić tę wartość, aby użyć innego serwera sieci Web do obsługi żądań informacji o zdarzeniach.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Program events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Parametry wiersza polecenia programu events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Adres URL events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
