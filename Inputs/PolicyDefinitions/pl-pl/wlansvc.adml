      <presentation id="SetCost_Presentation">
        <dropdownList refId="Cost_Dropdownlist" defaultItem="0">Wybierz wartość kosztu połączenia z siecią WLAN, która ma zostać ustawiona:</dropdownList>
      </presentation>
    </presentationTable>
