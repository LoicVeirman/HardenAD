        <decimalTextBox refId="Interval" defaultValue="1" spinStep="1" xml:space="preserve">보고 반복 간격(일수)              </decimalTextBox>
        <decimalTextBox refId="Data_Cache_Limit" defaultValue="20" spinStep="10" xml:space="preserve">데이터 캐시 제한               </decimalTextBox>
