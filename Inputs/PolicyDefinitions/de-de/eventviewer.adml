<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>Anzeigenamen hier eingeben</displayName>
  <description>Beschreibung hier eingeben</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Ereignisanzeige</string>
      <string id="EventViewer_RedirectionProgram">Events.asp-Programm</string>
      <string id="EventViewer_RedirectionProgram_Help">Dies ist das beim Klicken auf den events.asp-Link ausgeführte Programm.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Events.asp-Programm-Befehlszeilenparameter</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Dies gibt die Kommandozeilenparameter an, die an das "events.asp"-Programm weitergegeben werden.</string>
      <string id="EventViewer_RedirectionURL">Events.asp-URL</string>
      <string id="EventViewer_RedirectionURL_Help">Dies ist die URL, die an den Beschreibungsbereich im Dialogfeld "Ereigniseigenschaften" übergeben wird. Ändern Sie diesen Wert, wenn ein anderer Webserver zur Verarbeitung von Ereignisinformationsanforderungen verwendet werden soll.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Events.asp-Programm</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Events.asp-Programm-Befehlszeilenparameter</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Events.asp-URL</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
