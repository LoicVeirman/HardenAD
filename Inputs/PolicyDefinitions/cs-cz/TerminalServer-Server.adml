   <presentationTable>
      <presentation id="TS_SESSIONS_RemoteApp_End_Timeout">
        <dropdownList refId="TS_SESSIONS_RemoteApp_End_Timeout" noSort="true" defaultItem="0">Zpoždění odhlášení relace aplikace RemoteApp:</dropdownList>
      </presentation>
      <presentation id="TS_NET_ID_IP_VIRTUALIZATION">
	    <textBox refId="TS_NET_ID_IP_VIRTUALIZATION">
	      <label>Definujte IP adresu a masku sítě odpovídající síťovému adaptéru pro Virtualizaci IP vzdálené plochy:</label>
	    </textBox>
    	<text>Formát: [IP adresa]/[maska sítě]. Například: 192.0.2.96/24</text>
      </presentation>
      <presentation id="TS_ENABLE_IP_VIRTUALIZATION">
