      <string id="CategoryCompatView">Kompatibilní zobrazení</string>
      <string id="CompatView_AllSites">Zapnout režim standardů aplikace Internet Explorer 7</string>
      <string id="CompatView_DisableList">Vypnout kompatibilní zobrazení</string>
      <string id="CompatView_IntranetSites">Zapnout režim standardů aplikace Internet Explorer pro místní intranet</string>
