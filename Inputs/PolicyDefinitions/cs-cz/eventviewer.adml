<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>Zde zadejte zobrazovaný název.</displayName>
  <description>Zde zadejte popis.</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Prohlížeč událostí</string>
      <string id="EventViewer_RedirectionProgram">Program souboru Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Určuje program, který bude spuštěn při kliknutí na odkaz souboru events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Parametry příkazového řádku programu events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Určuje parametry příkazového řádku, které budou předány programu pro soubor events.asp.</string>
      <string id="EventViewer_RedirectionURL">Adresa URL souboru Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Toto je adresa URL, která bude zobrazena v popisu události v dialogu Vlastnosti události. Pokud chcete ke zpracování požadavků na informace o událostech použít jiný webový server, změňte toto nastavení.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Program souboru Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Parametry příkazového řádku programu events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Adresa URL souboru Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
