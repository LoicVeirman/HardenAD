        <checkBox refId="PutDataOwnerOnToCheck" defaultChecked="true">Kansion omistaja</checkBox>
        <checkBox refId="PutAdminOnToCheck" defaultChecked="true">Tiedostopalvelimen järjestelmänvalvoja</checkBox>
        <textBox refId="AdditonalEmailToText">
          <label>Muut vastaanottajat:</label>
        </textBox>
        <text> </text>
        <text>Sähköpostiasetukset:</text>
        <checkBox refId="IncludeDeviceClaimsCheck" defaultChecked="true">Sisällytä laiteväitteet</checkBox>
        <checkBox refId="IncludeUserClaimsCheck" defaultChecked="true">Sisällytä käyttäjäväitteet</checkBox>
        <checkBox refId="GenerateLogCheck" defaultChecked="true">Kirjaa sähköpostit sovellusten ja palveluiden lokeihin</checkBox>
      </presentation>
    </presentationTable>
  </resources>
