      <string id="TS_RemoteDesktopVirtualGraphics_ScreenCaptureRate_Medium">Keskitaso (oletus)</string>
      <string id="TS_RemoteDesktopVirtualGraphics_ScreenCaptureRate_Lowest">Pienin</string>
      <string id="TS_RemoteDesktopVirtualGraphics_ScreenImageQuality_Highest">Suurin (paras laatu)</string>
