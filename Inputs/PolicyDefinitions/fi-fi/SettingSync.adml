      </presentation>

      <!-- Credentials -->
      <presentation id="DisableCredentialsSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa salasanojen synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- Personalization -->
      <presentation id="DisablePersonalizationSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa mukautusten synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- AppSync -->
      <presentation id="DisableAppSyncSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa AppSync-synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- Windows -->
      <presentation id="DisableWindowsSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa muiden Windowsin asetusten synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- Desktop Theme -->
      <presentation id="DisableDesktopThemeSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa työpöydän mukauttamisen synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- WebBrowser -->
      <presentation id="DisableWebBrowserSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa selaimen synkronointi käyttöön.</checkBox>
      </presentation>
      
      <!-- StartLayout -->
      <presentation id="DisableStartLayoutSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa aloitusnäytön asettelun synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- AccessibilitySync -->
      <presentation id="DisableAccessibilitySettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa "helppokäyttötoimintojen" synkronointi käyttöön.</checkBox>
      </presentation>

      <!-- LanguageSync -->
      <presentation id="DisableLanguageSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Salli käyttäjien ottaa kieliasetusten synkronointi käyttöön.</checkBox>
      </presentation>

    </presentationTable>
  </resources>
