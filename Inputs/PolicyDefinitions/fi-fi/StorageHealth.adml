<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
<displayName>Tallennustilan kunto</displayName>
<description>Tallennustilan kunnon ADMX-tiedosto</description>
<resources>
    <stringTable>
    <string id="StorageHealth">Tallennustilan kunto</string>
    <string id="SH_AllowDiskHealthModelUpdates">Salli päivitysten lataaminen levyvirheen ennakointimalliin</string>
    <string id="SH_AllowDiskHealthModelUpdates_Explain">
        Mahdollistaa uusien päivitysten lataamisen ML-mallin parametreihin tallennustilan levyvirheiden ennakointia varten.

        Käytössä:
        Päivitykset ladataan levyvirheen ennakointivirhemalliin.

        Ei käytössä:
        Päivityksiä ei ladata levyvirheen ennakointivirhemalliin.

        Ei määritetty:
        Sama kuin Käytössä.
    </string>
    </stringTable>
</resources>
