    <string id="Root_UpdateChannel_Dropdown_Preview">Aktuell kanal (förhandsversion)</string>
    <string id="Root_UpdateChannel_Dropdown_Staged">Aktuell kanal (mellanlagrad)</string>
    <string id="Root_UpdateChannel_Dropdown_Broad">Aktuell kanal (bred)</string>
