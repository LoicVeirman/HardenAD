        <decimalTextBox refId="BITS_MaxTransferRateText" defaultValue="10" spinStep="10">Περιορισμός ταχύτητας μεταφοράς παρασκηνίου (Kbps) σε:</decimalTextBox>
        <dropdownList refId="BITS_BandwidthLimitSchedFrom" noSort="true" defaultItem="8">Από</dropdownList>
