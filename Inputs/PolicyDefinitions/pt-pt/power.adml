      <string id="DCBatteryDischargeAction1">Ação de notificação de bateria fraca</string>
      <string id="DCBatteryDischargeLevel0">Nível de notificação de bateria muito fraca</string>
      <string id="DCBatteryDischargeLevel1">Nível de notificação de bateria fraca</string>
      <string id="DCBatteryDischargeLevel1UINotification">Desativar notificação de utilizador de bateria fraca</string>
