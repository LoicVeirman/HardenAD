    <stringTable>
      <string id="MMC">Microsoft Management Console</string>
      <string id="MMC_ActiveXControl">ActiveX 控制項</string>
      <string id="MMC_ExtendView">延伸檢視 (網頁檢視)</string>
      <string id="MMC_ExtensionSnapins">延伸嵌入式管理單元</string>
