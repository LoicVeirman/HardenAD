      <string id="TS_TIME_16HR">16 timer</string>
      <string id="TS_TIME_18HR">18 timer</string>
      <string id="TS_TIME_1DAY">1 dag</string>
      <string id="TS_TIME_2DAY">2 dager</string>
      <string id="TS_TIME_3DAY">3 dager</string>
