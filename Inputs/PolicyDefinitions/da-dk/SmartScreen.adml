      <string id="ShellConfigureSmartScreen">Konfigurer Windows Defender SmartScreen</string>
      <string id="SmartScreen_PreventBypass">Advar, og undgå tilsidesættelse</string>
      <string id="SmartScreen_Warn">Advar</string>
