      <string id="AM_SetFileRiskLevel">Standard-risikoniveau for vedhæftede filer</string>
      <string id="AM_SetHighRiskInclusion">Medtagelseslisten for filtyper med høj risiko</string>
      <string id="AM_SetLowRiskInclusion">Medtagelseslisten for filtyper med lav risiko</string>
      <string id="AM_SetModRiskInclusion">Medtagelseslisten for filtyper med mellem risiko</string>
    </stringTable>
