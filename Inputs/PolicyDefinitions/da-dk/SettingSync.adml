      </presentation>

      <!-- WebBrowser -->
      <presentation id="DisableWebBrowserSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Tillad brugerne at slå synkronisering af browsere til.</checkBox>
      </presentation>
      
      <!-- StartLayout -->
      <presentation id="DisableStartLayoutSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Tillad brugerne at slå synkronisering af "startlayout" til.</checkBox>
      </presentation>

      <!-- AccessibilitySync -->
      <presentation id="DisableAccessibilitySettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Tillad brugerne at slå synkronisering af "tilgængelighed" til.</checkBox>
      </presentation>

      <!-- LanguageSync -->
      <presentation id="DisableLanguageSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Tillad brugere at slå synkronisering af sprogindstillinger til.</checkBox>
      </presentation>

    </presentationTable>
  </resources>
</policyDefinitionResources>
