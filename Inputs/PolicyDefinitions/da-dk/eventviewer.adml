<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>indtast det viste navn her</displayName>
  <description>indtast beskrivelsen her</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Logbog</string>
      <string id="EventViewer_RedirectionProgram">Events.asp-program</string>
      <string id="EventViewer_RedirectionProgram_Help">Dette er det program, der startes, når brugeren klikker på events.asp-hyperlinket.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Kommandolinjeparametre til Events.asp-program</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Dette angiver de kommandolinjeparametre, der overføres til events.asp-programmet</string>
      <string id="EventViewer_RedirectionURL">Events.asp-URL-adresse</string>
      <string id="EventViewer_RedirectionURL_Help">Dette er den URL-adresse, der overføres til området Beskrivelse i dialogboksen Egenskaber for Hændelse. Ret værdien, hvis du vil bruge en anden webserver til at håndtere anmodninger om hændelsesoplysninger.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Events.asp-program</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Kommandolinjeparametre til Events.asp-program</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Events.asp-URL-adresse</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
