<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>buraya görünen adı girin</displayName>
  <description>buraya tanımı girin</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Olay Görüntüleyicisi</string>
      <string id="EventViewer_RedirectionProgram">Events.asp programı</string>
      <string id="EventViewer_RedirectionProgram_Help">Bu, kullanıcı events.asp bağlantısını tıklattığında başlatılacak olan programdır.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Events.asp programı komut satırı parametreleri</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Bu, events.asp programına geçirilecek olan komut satırı parametrelerini belirtir</string>
      <string id="EventViewer_RedirectionURL">Events.asp URL'si</string>
      <string id="EventViewer_RedirectionURL_Help">Bu, Olay Özellikleri iletişim kutusunda Açıklama alanına geçirilecek olan URL'dir. Olay bilgisi isteklerini işlemek üzere başka bir Web sunucusu kullanmak isterseniz bu değeri değiştirin.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Events.asp programı</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Events.asp programı komut satırı parametreleri</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Events.asp URL'si</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
