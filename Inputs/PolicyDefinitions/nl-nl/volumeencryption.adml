      <string id="OSRecoveryPasswordUsageDropDown_Allow">Herstelwachtwoord van 48 cijfers toestaan</string>
      <string id="OSRecoveryPasswordUsageDropDown_Disallow">Herstelwachtwoord van 48 cijfers niet toestaan</string>

