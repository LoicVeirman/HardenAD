<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>voer hier de weergegeven naam in</displayName>
  <description>typ hier een beschrijving</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Logboeken</string>
      <string id="EventViewer_RedirectionProgram">Programma Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Dit is het programma dat wordt gestart als de gebruiker op de koppeling events.asp klikt.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Opdrachtregelparameters voor het programma Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Hiermee worden de opdrachtregelparameters opgegeven die worden doorgegeven aan het programma Events.asp.</string>
      <string id="EventViewer_RedirectionURL">URL Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Dit is de URL die wordt doorgegeven aan het gebied Beschrijving in het dialoogvenster Eigenschappen van gebeurtenis. U kunt deze waarde wijzigen als u een andere webserver wilt gebruiken voor het verwerken van aanvragen voor gebeurtenisgegevens.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Programma Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Opdrachtregelparameters voor het programma Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>URL Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
