<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>immettere qui il nome visualizzato</displayName>
  <description>immettere qui la descrizione</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Visualizzatore eventi</string>
      <string id="EventViewer_RedirectionProgram">Programma Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Programma che verrà richiamato quando l'utente fa clic sul collegamento a events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Parametri della riga di comando per Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Specifica i parametri della riga di comando che vengono passati al programma events.asp</string>
      <string id="EventViewer_RedirectionURL">URL Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Questo è l'URL inviato all'area Descrizione nella finestra di dialogo Proprietà evento. Cambiare questo valore se si desidera utilizzare un altro server Web per gestire le richieste di informazioni sugli eventi.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Programma Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Parametri della riga di comando per Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>URL Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
