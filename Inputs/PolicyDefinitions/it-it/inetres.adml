      <string id="Help_NoFeedback">Menu ?: rimuove l'opzione di menu Commenti e suggerimenti</string>
      <string id="Help_NoNetscapeHelp">Menu ?: rimuove l'opzione di menu Per utenti Netscape</string>
      <string id="Help_NoTip">Menu ?: rimuove l'opzione di menu Suggerimenti utili</string>
