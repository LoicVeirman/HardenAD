<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>itt adja meg a megjelenítendő nevet</displayName>
  <description>itt adja meg a leírást</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Eseménynapló</string>
      <string id="EventViewer_RedirectionProgram">Az Events.asp program</string>
      <string id="EventViewer_RedirectionProgram_Help">Ez az a program, amelyet a rendszer akkor hív meg, amikor a felhasználó az events.asp hivatkozására kattint.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Az Events.asp program parancssori kapcsolói</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Azok a parancssori kapcsolók, amelyeket a rendszer az events.asp programnak átad.</string>
      <string id="EventViewer_RedirectionURL">Events.asp URL-címe</string>
      <string id="EventViewer_RedirectionURL_Help">Ez az az URL-cím, amelyet a rendszer az Esemény tulajdonságait tartalmazó párbeszédpanel Leírás területébe továbbít. Ha az eseményadatokra vonatkozó kéréseket egy másik webkiszolgálóval kívánja kezelni, módosítsa ezt az értéket.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Az Events.asp program</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Az Events.asp program parancssori kapcsolói</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Events.asp URL-címe</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
