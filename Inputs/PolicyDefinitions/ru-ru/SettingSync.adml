      </presentation>

      <!-- LanguageSync -->
      <presentation id="DisableLanguageSettingSyncUserOverride">
        <checkBox refId="CheckBox_UserOverride">Разрешить пользователям включить синхронизацию "языковых настроек".</checkBox>
      </presentation>

    </presentationTable>
