<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>введите выводимое имя</displayName>
  <description>введите описание</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Просмотр событий</string>
      <string id="EventViewer_RedirectionProgram">Программа Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Эта программа, запускаемая щелчком ссылки events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Параметры командной строки программы Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Задает параметры командной строки, передаваемые программе Events.asp.</string>
      <string id="EventViewer_RedirectionURL">URL-адрес программы Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Этот URL-адрес будет передан в область описания диалогового окна свойств событий. Для использования другого веб-сервера обработки запросов информации событий измените это значение.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Программа Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Параметры командной строки программы Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>URL-адрес программы Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
