      <string id="AllowUsingDiacritics">允许使用音调符号</string>
      <string id="Win7Only">Microsoft Windows 7 或更高版本</string>
      <string id="WS4Only">具有 Windows Search 4.0 的任何 Microsoft Windows 版本</string>
      <string id="Win8Only">Microsoft Windows 8 或更高版本</string>
      <string id="WinBlueOnly">Microsoft Windows 8.1 或更高版本</string>
