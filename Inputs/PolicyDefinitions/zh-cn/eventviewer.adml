<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>请在此处输入显示名称</displayName>
  <description>请在此处输入描述</description>
  <resources>
    <stringTable>
      <string id="EventViewer">事件查看器</string>
      <string id="EventViewer_RedirectionProgram">Events.asp 程序</string>
      <string id="EventViewer_RedirectionProgram_Help">当用户单击 events.asp 链接时将调用此程序。</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Events.asp 程序命令行参数</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">指定传递给 events.asp 程序的命令行参数</string>
      <string id="EventViewer_RedirectionURL">Events.asp URL</string>
      <string id="EventViewer_RedirectionURL_Help">此 URL 将传递到“事件属性”对话框中的“描述”域。如果需要使用其他 Web 服务器处理事件信息请求，请更改这个值。</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Events.asp 程序</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Events.asp 程序命令行参数</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>Events.asp URL</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
