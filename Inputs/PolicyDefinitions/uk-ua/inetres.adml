      <string id="Protocols_SSL3_TLS11_TLS12">Use SSL 3.0, TLS 1.1, and TLS 1.2</string>
      <string id="Protocols_SSL2_SSL3_TLS11_TLS12">Use SSL 2.0, SSL 3.0, TLS 1.1, and TLS 1.2</string>
