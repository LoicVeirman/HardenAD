      <string id="Protocols_SSL2_TLS10_TLS12">Use SSL 2.0, TLS 1.0, and TLS 1.2</string>
      <string id="Protocols_SSL3_TLS10_TLS12">Use SSL 3.0, TLS 1.0, and TLS 1.2</string>
