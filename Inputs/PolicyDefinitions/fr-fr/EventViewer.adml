<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>tapez le nom complet ici</displayName>
  <description>tapez la description ici</description>
  <resources>
    <stringTable>
      <string id="EventViewer">Observateur d’événements</string>
      <string id="EventViewer_RedirectionProgram">Programme Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">Ce programme est invoqué lorsque l’utilisateur clique sur le lien events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">Paramètres de ligne de commande du programme Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">Spécifie les paramètres de ligne de commande qui sont passés au programme events.asp</string>
      <string id="EventViewer_RedirectionURL">URL Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">Cette URL est passée à la zone Description dans la boîte de dialogue Propriétés de l’événement. Modifiez cette valeur si vous souhaitez utiliser un autre serveur Web pour traiter les requêtes d’informations d’événements.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>Programme Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>Paramètres de ligne de commande du programme Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>URL Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
