      <string id="IESF_CategoryMKProtocolSecurityRestriction">Restriction de sécurité du protocole MK</string>
      <string id="IESF_CategoryNetworkProtocolLockdown">Verrouillage des protocoles réseau</string>
      <string id="IESF_CategoryObjectCachingProtection">Protection de mise en cache d’objets</string>
